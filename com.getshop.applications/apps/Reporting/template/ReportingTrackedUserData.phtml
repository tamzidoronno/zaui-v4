        <th><? echo $this->__f("Additional info"); ?></th>
    </tr>
    
    <?
    foreach($results as $result) {
      /* @var $result core_reportingmanager_data_EventLog */
        echo "<tr>";
        echo "<td>" . $result->when . "</td>";
        echo "<td>" . $result->method . "</td>";
        echo "<td>" . $result->additionalText . "</td>";
        echo "</tr>";
    }
    ?>
</table>