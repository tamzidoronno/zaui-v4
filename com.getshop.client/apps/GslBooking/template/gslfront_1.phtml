<script>

//Reset page on load.
    $(function() {
        sessionStorage.setItem('gslcurrentpage','');
        $('.gslbookingBody').hide();
        $('.addons_overview').hide();
       $.getScript( "{getshop_endpoint}/scripts/booking/bookingscripts.php", function( data, textStatus, jqxhr ) {        
        $.getScript( "{getshop_endpoint}/js/daterangepicker/moment.js", function( data, textStatus, jqxhr ) {
            $.getScript( "{getshop_endpoint}/js/daterangepicker/daterangepicker.js", function( data, textStatus, jqxhr ) {
                getshop_setBookingTranslation();
                var getshop_waitforload = setInterval(function() {
                    if(typeof(getshop_bookingconfiguration) === "undefined") {
                        return;
                    }
                    getshop_setDatePicker();
                    clearInterval(getshop_waitforload);
                }, 50);
            });
        });
       });
    });
</script>
