<script>
function loadGallery(){
    var gallery = document.querySelector('.gallery');
    var itemWidth = 23;
    var leftInterval;
    var rightInterval;
    var scrollRate = 0.2;
    var left;

    $(document).on('click', '.gallery-item', function(e){
        var featured = $(this).closest('.roomimg');
        featured.find('.gallery-item').removeClass('active');
        if (!$(this).hasClass('active')){
            $(this).addClass('active');
            featured.find('.featured-item').css('backgroundImage', e.target.style.backgroundImage);
        }
    });
    $(document).on('mouseenter', '.move-btn.left' ,function(){
        left = left || 0;
        var thisGallery = $(this).closest('.roomimg').find('.gallery'); 
        leftInterval = setInterval(function() {
            thisGallery.css('left', left + '%');
            if (left > -itemWidth) {
                left -= scrollRate;
            } else {
                left = 0;
                
                //Change to jquery var 4 lines under!
                var first = gallery.children[0];
                gallery.removeChild(first);
                gallery.style.left = -itemWidth + '%';
                gallery.appendChild(first);
                gallery.style.left = '0%';
                
//                var first = $('.gallery item-wrapper:first-child');
//                first.detach();
//                thisGallery.css('left',-itemWidth + '%');
//                thisGallery.append(first);
//                thisGallery.css('left', '0%');
//                console.log(first);
            }
        }, 1);
    });
    $(document).on('mouseenter', '.move-btn.right', function(){
        var thisGallery = $(this).closest('.roomimg').find('.gallery'); 
        if (left > -itemWidth && left < 0) {
            left = left  - itemWidth;
            
            //Change to jquery var 3 lines under!
            var last = gallery.children[gallery.children.length - 1];
            gallery.removeChild(last);
            gallery.insertBefore(last, gallery.children[0]);
            
            thisGallery.css('left', left + '%');
        }
        left = left || 0;
        leftInterval = setInterval(function() {
            thisGallery.css('left', left + '%');

            if (left < 0) {
                left += scrollRate;
            } else {
                left = -itemWidth;
                
                //Change to jquery var 4 lines under!
                var last = gallery.children[gallery.children.length - 1];
                gallery.removeChild(last);
                gallery.insertBefore(last, gallery.children[0]);
                gallery.style.left = '-23%';
            }
        }, 1);
    });
    $(document).on('mouseleave', '.move-btn',function(){
        clearInterval(leftInterval);
        clearInterval(rightInterval);
    });
}
</script>

