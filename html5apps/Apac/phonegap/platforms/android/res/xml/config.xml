<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="100183" id="com.getshop.trackandtrace" version="1.0.31" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="HoneyWellBarcodeReaderE75">
        <param name="android-package" value="no.hsnews.e75.BarcodeReader75E" />
    </feature>
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="SQLitePlugin">
        <param name="android-package" value="io.sqlc.SQLitePlugin" />
    </feature>
    <allow-intent href="market:*" />
    <name>Track And Trace</name>
    <description>
        A app to track delivery and pickups of packages.
    </description>
    <author email="kai@getshop.com" href="http://www.getshop.no">
        GetShop Team
    </author>
    <preference name="orientation" value="portrait" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
